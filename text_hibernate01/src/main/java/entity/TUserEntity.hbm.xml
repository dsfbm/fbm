<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.TUserEntity" table="t_user" schema="springboot">
        <id name="uid">
            <column name="uid" sql-type="int(45)"/>
        </id>
        <property name="username">
            <column name="username" sql-type="varchar(34)" length="34"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(34)" length="34"/>
        </property>
    </class>
</hibernate-mapping>