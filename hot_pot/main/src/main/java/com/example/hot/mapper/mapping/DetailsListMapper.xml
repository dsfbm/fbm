<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.hot.mapper.DetailsListMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.hot.entity.DetailsList">
    <result column="id" property="id" />
    <result column="flag" property="flag" />
    <result column="create_time" property="createTime" />
    <result column="update_time" property="updateTime" />
        <result column="store_id" property="storeId" />
        <result column="order_id" property="orderId" />
        <result column="food_id" property="foodId" />
        <result column="food_count" property="foodCount" />
        <result column="food_price" property="foodPrice" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        flag,
        create_time,
        update_time,
        store_id, order_id, food_id, food_count, food_price
    </sql>

</mapper>
