<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.hot.mapper.OrderListMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.hot.entity.OrderList">
    <result column="id" property="id" />
    <result column="flag" property="flag" />
    <result column="create_time" property="createTime" />
    <result column="update_time" property="updateTime" />
        <result column="store_id" property="storeId" />
        <result column="customer_id" property="customerId" />
        <result column="order_price" property="orderPrice" />
        <result column="food_count" property="foodCount" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        flag,
        create_time,
        update_time,
        store_id, customer_id, order_price, food_count
    </sql>

</mapper>
